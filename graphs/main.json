{
  "properties": {
    "name": "main",
    "environment": {
      "type": "msgflo"
    }
  },
  "processes": {
    "airportweather": {
      "component": "c3-flo/AirportWeather"
    },
    "clock": {
      "component": "c3-flo/Clock"
    },
    "farbgeber": {
      "component": "c3-flo/Farbgeber"
    },
    "launch-announcer": {
      "component": "c3-flo/LaunchAnnouncer"
    },
    "station-announcer": {
      "component": "station-announcer/StationAnnouncer"
    },
    "visual-paging": {
      "component": "c3-flo/VisualPaging"
    },
    "north-display": {
      "component": "c-base/infodisplay"
    },
    "east-display": {
      "component": "c-base/infodisplay"
    },
    "south-display": {
      "component": "c-base/infodisplay"
    },
    "west-display": {
      "component": "c-base/infodisplay"
    },
    "mainhall-display": {
      "component": "c-base/infodisplay"
    },
    "c-lab-display": {
      "component": "c-base/infodisplay"
    },
    "roof-panels": {
      "component": "c3-flo/McLightings"
    }
  },
  "connections": [
    {
      "data": [
        "ws://rpne1.35c3.c-base.org:81/",
        "ws://rpne2.35c3.c-base.org:81/",
        "ws://rpse1.35c3.c-base.org:81/",
        "ws://rpse2.35c3.c-base.org:81/",
        "ws://rpsw1.35c3.c-base.org:81/",
        "ws://rpsw2.35c3.c-base.org:81/",
        "ws://rpnw1.35c3.c-base.org:81/",
        "ws://rpnw2.35c3.c-base.org:81/"
      ],
      "tgt": {
        "process": "roof-panels",
        "port": "addresses"
      }
    },
    {
      "data": "EDDP",
      "tgt": {
        "process": "airportweather",
        "port": "icao"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "clock",
        "port": "start"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "airportweather",
        "port": "fetch"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "launch-announcer",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "launch-announcer",
        "port": "out"
      },
      "tgt": {
        "process": "visual-paging",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "station-announcer",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "station-announcer",
        "port": "out"
      },
      "tgt": {
        "process": "visual-paging",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "north-display",
        "port": "open"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "east-display",
        "port": "open"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "south-display",
        "port": "open"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "west-display",
        "port": "open"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "mainhall-display",
        "port": "open"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "c-lab-display",
        "port": "open"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "farbgeber",
        "port": "in"
      }
    },
    {
      "data": 4,
      "tgt": {
        "process": "station-announcer",
        "port": "probability"
      }
    }
  ]
}

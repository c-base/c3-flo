{
  "properties": {
    "name": "main",
    "environment": {
      "type": "msgflo"
    }
  },
  "processes": {
    "airportweather": {
      "component": "c3-flo/AirportWeather"
    },
    "clock": {
      "component": "c3-flo/Clock"
    },
    "farbgeber": {
      "component": "c3-flo/farbgeber"
    },
    "launch-announcer": {
      "component": "c3-flo/LaunchAnnouncer"
    }
  },
  "connections": [
    {
      "data": "EDDP",
      "tgt": {
        "process": "airportweather",
        "port": "icao"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "clock",
        "port": "start"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "airportweather",
        "port": "fetch"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "launch-announcer",
        "port": "in"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "farbgeber",
        "port": "in"
      }
    }
  ]
}

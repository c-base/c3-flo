{
  "properties": {
    "name": "main",
    "environment": {
      "type": "msgflo"
    }
  },
  "processes": {
    "airportweather": {
      "component": "c3-flo/AirportWeather"
    },
    "clock": {
      "component": "c3-flo/Clock"
    },
    "demodisplay": {
      "component": "c-base/infodisplay"
    },
    "farbgeber": {
      "component": "c3-flo/farbgeber"
    },
    "launch-announcer": {
      "component": "c3-flo/LaunchAnnouncer"
    },
    "station-announcer": {
      "component": "station-announcer/StationAnnouncer"
    },
    "visual-paging": {
      "component": "c3-flo/VisualPaging"
    }
  },
  "connections": [
    {
      "data": "EDDP",
      "tgt": {
        "process": "airportweather",
        "port": "icao"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "clock",
        "port": "start"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "airportweather",
        "port": "fetch"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "launch-announcer",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "launch-announcer",
        "port": "out"
      },
      "tgt": {
        "process": "visual-paging",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "clock",
        "port": "out"
      },
      "tgt": {
        "process": "station-announcer",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "station-announcer",
        "port": "out"
      },
      "tgt": {
        "process": "visual-paging",
        "port": "in"
      }
    },
    {
      "src": {
        "process": "visual-paging",
        "port": "out"
      },
      "tgt": {
        "process": "demodisplay",
        "port": "open"
      }
    },
    {
      "data": true,
      "tgt": {
        "process": "farbgeber",
        "port": "in"
      }
    },
    {
      "data": 3,
      "tgt": {
        "process": "station-announcer",
        "port": "probability"
      }
    }
  ]
}
